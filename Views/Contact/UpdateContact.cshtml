@model AcunmedyaAkademiPortfolio.Models.TblContact

@{
    ViewBag.Title = "UpdateContact";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<link href="~/Content/bootstrap.min.css" rel="stylesheet" />


<h2>İletişim Güncelleme</h2>
<form method="post">
    @Html.HiddenFor(x => x.ContactId)

    <label>Adress</label>
    @Html.TextBoxFor(x => x.Adress, new { @class = "form-control" })

    <label>E-Mail</label>
    @Html.TextBoxFor(x => x.Email, new { @class = "form-control" })

    <label>Telefon</label>
    @Html.TextBoxFor(x => x.Phone, new { @class = "form-control" })

    <button class="btn btn-info">İletişimi Kaydet</button>
</form>